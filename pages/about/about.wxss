.user-nickname{
  text-align: center;display: block;
}
.user-avatar{width: 80px;height: 80px;border-radius: 50%;display: block;}
.avatar-box{
  border: 1px solid gainsboro;
  padding: 10rpx;
  box-shadow: 0 0 10rpx ;
  border-radius: 50%;
}
.title{display: block;text-align: center;color: #333;margin-top: 10px;}
.linkbox{
  display: flex;
  padding: 10px 10%;
}
.linkbox navigator{
  flex: 1;
  text-align: center;
  color: gray;
  font-size: small;
}
.linklist{
  padding: 10px 5%;
}
.linklist navigator,button{
  color: gray;
  font-size: small;
  display: block;
  padding: 10px 6%;
  border-bottom: 1px solid  gainsboro;
  text-align: left;
  position: relative;
}
 
.icon,.rightar{
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: bottom;
}
.rightar{position: absolute;
 right: 5px;
 bottom: 5px;
}
.icon.s{
  width: 18px;height: 18px;
}
.cs_button{
  padding-top: 0;
  padding-bottom: 0;
  background-color: white;
  border-radius: 0;
  position: relative;
  display: block;
}
 .cs_button .icon{
  vertical-align:middle;
 }

.linklist navigator:after{
  content: '';
  position: absolute;
  width: 20px;
  height: 20px;
  right:5px;
  background-size: contain;
  background-image: url('data:image/png;base64,')
}
button.cs_button:after{
  border-radius: 0;
  border-width: 0 0 0 0;
}
button:active{
  background-color: gainsboro;
}